//single inheritance
package pack5;

public class parent {
	
	public void tnagar() {
		System.out.println("property");
	}
	public void mettukuppam() {
		System.out.println("jewels");
	}
	
	public static void main(String[] args) {
		
		parent p = new parent();
		p.tnagar();
		p.mettukuppam();
		
	}

}

//different class 

package pack5;

public class child extends parent {
	
	public void phone() {
		System.out.println("redmi");
	}
	
	public void bike() {
		System.out.println("duke");
	}
	
	public static void main(String[] args) {
		
		child c = new child();
		c.phone();
		c.bike();
		
		c.tnagar();
		c.mettukuppam();
			
		
	}
}

multi  level inheritance

package pack5;

public class grandparent {
	
	public void food() {
		System.out.println("snacks");
	}

}


package pack5;

public class child extends parent {
	
	public void phone() {
		System.out.println("redmi");
	}
	
	public void bike() {
		System.out.println("duke");
	}
	
	public static void main(String[] args) {
		
		child c = new child();
		c.phone();
		c.bike();
		
		c.tnagar();
		c.mettukuppam();
			
		c.food();
	}
}

//hierachical inheritance

package pack4;

public class Mom {
	
	public void food() {
		System.out.println("Biriyani");
	}
	public void saree() {
		System.out.println("silk");
	}
	public void jewels() {
		System.out.println("gold");
	}

}
//child1 class

package pack4;

public class Child1 extends Mom{
	
	public void amazon() {
		System.out.println("bags");
		
	}
	public static void main(String[] args) {
		
		Child1 c = new Child1();
		c.amazon();
		c.food();
		
	}

}
//child2 class

package pack4;

public class Child2 extends Mom{
	public void flipcart() {
		System.out.println("watch");
	}
	public static void main(String[] args) {
		
		Child2 c = new Child2();
		c.flipcart();
		c.jewels();
		c.saree();
		
	}
}


